1.
class rectangle
{
public static void main(String args[])
{
int l=5,b=7,a,p;
a=l*b;
p=2*(l+b)
System.out.println("area of rectangle is",+a);
System.out.println("perimater of rectangle is",+p);
}
}

2.
class triangle
{
public static void main(String args[])
{
int a=2,b=3,c=5,p;
p=a+b+c;
System.out.println("perimater of triangle is",+p);
}
}


