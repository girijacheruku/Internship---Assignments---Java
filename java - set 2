13.
class Main
{
public static void main(String args[])
{
int n=12345;
int s=0;
for(int i=0;i<5;i++)
{
if(i==1 || i==4)
{
int p=n%10;
s=s+p;
}
n=n/10;
}
System.out.println(s);
}
}


14.
class Main
{
public static void main(String args[])
{
int n=1234;
int s=0;
int t=n;
for(int i=3;i>=0;i--)
{
int p=(t/(10^i))-(s*10);
p=p+2;
s=(s*10)+p;
}
System.out.println(s);
}
}


