1.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int a;
        a = 15;
        double b;
        b= 20.5;
        char s;
        s = 'd';
        System.out.println(a);
        System.out.println(b);
        System.out.println(s);
	}
}

2.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	int l=2;  
    int b=3;  
    int a=l*b;  
    System.out.println("Area of rectangle :"+a);  
	}
}

3.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	double a=8.2;  
    int b=6; 
    System.out.println("product :"+a*b);
	}
}

4.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	char s = 'h';
    System.out.println("The ASCII value of h is: "+(int)s);
	}
}

5.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	double d = 100.235;
	System.out.println((int)d); 
	}
}

6.

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	char s='d';
        s=(char)(s+3);
        System.out.println(s);
	}
}

7.

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	double a=6.2;  
    int b=5;
    double c=a+b;  
    System.out.println("addition of a and b:"+c);
	}
}

